PREFIX cat: <http://cat.nationalarchives.gov.uk/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX prov: <http://www.w3.org/ns/prov#>

SELECT  ?recordDescriptionUri ?label ?identifier
WHERE
{ ?recordDescriptionUri prov:specializationOf  cat:COAL.2022.N373.P ;
                        dct:relation ?relation .
  ?relation dct:type ?relationType ;
            dct:title ?label ;
            dct:subject ?identifier .
}