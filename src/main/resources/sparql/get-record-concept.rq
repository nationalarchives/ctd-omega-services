PREFIX cat: <http://cat.nationalarchives.gov.uk/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX ver: <http://purl.org/linked-data/version#>

SELECT ?recordConceptUri ?formatUri ?currentDescriptionUri
WHERE { ?recordConceptUri dct:identifier ?recordConceptId ;
                          dct:format ?formatUri ;
                          ver:currentVersion ?currentDescriptionUri }
